# solution 1
for i in list(input().lower()):
    if i not in list("aeiouy"):
        print(f".{i}",end='')
print(end='\n')
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++









++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
# solution 2
word= input()
vowels= {'A','a','E','e','Y','y','I','i','O','o','U','u'}
for i in word.lower():
    if i in vowels:
        pass
    else:
        print('.'+i,end='')
        
        
